{
    "name": "smetdenis/jbdump",
    "type": "library",
    "description": "Script for debug and dump PHP variables and other stuff. This tool is a nice replacement for print_r() and var_dump() functions.",
    "license": "MIT",
    "keywords": [
        "jbdump",
        "dump",
        "php",
        "dumper",
        "debug",
        "debugging",
        "pretty",
        "print",
        "print_r",
        "var_dump",
        "vars"
    ],
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "SmetDenis",
            "email": "admin@jbzoo.com"
        }
    ],
    "require": {
        "php": ">=5.3.10"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "resources/global/class.php",
            "resources/global/function.php"
        ]
    },
    "bin": [
        "bin/jbdump"
    ]
}
