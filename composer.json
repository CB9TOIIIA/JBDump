{
    "name"             : "smetdenis/jbdump",
    "type"             : "library",
    "description"      : "Script for debug and dump PHP variables and other stuff. This tool is a nice replacement for print_r() and var_dump() functions.",
    "license"          : "MIT",
    "keywords"         : [
        "jbdump", "dump", "php", "dumper", "debug", "debugging", "pretty", "print", "print_r", "var_dump", "vars"
    ],
    "minimum-stability": "stable",
    "authors"          : [
        {
            "name" : "SmetDenis",
            "email": "admin@jbzoo.com"
        }
    ],
    "require"          : {
        "php"                : ">=5.3.10",
        "jdorn/sql-formatter": "dev-master"
    },
    "autoload"         : {
        "files": ["resources/functions/jbdump.php"],
        "psr-4": {
            "SmetDenis\\JBDump\\": "src/"
        }
    },
    "bin"              : ["bin/jbdump"]
}
